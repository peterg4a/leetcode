class Solution(object):
    def minEatingSpeed(self, piles, h):
        """
        :type piles: List[int]
        :type h: int
        :rtype: int
        """
        mx = int(10e9)
        mn = 0
        while mn+1 < mx:
            k = (mx+mn)/2
            hguess= 0
            #simulation
            for pile in piles:
                hguess += ceil(float(pile)/k)
            if hguess > h:
                mn = k
            else:
                mx = k
        return mx
            