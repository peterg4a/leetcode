class Solution(object):
    def findMin(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if nums[-1] >= nums[0]:
            return nums[0]
        l = 0
        r = len(nums)-1
        while l+1 < r:
            m = (l+r)/2
            # print(mn, mid, mx)
            if nums[m] >= nums[l]:
                l = m
            else:
                r = m
            # else:
            #     if nums[m] > nums[l]
        return nums[r]