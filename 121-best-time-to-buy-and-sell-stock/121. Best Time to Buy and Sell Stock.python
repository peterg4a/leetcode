class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        lowest = prices[0]
        maxprofit = 0
        for i in range(len(prices)):
            if prices[i] < lowest:
                lowest = prices[i]
            if prices[i] - lowest > maxprofit:
                maxprofit = prices[i] - lowest
        return maxprofit