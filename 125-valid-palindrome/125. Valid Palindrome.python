class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        letters = set(string.ascii_letters + string.digits)
        i = 0
        f = len(s) - 1
        while f > i:
            if s[i] in letters and s[f] in letters:
                if s[i].lower() != s[f].lower():
                    return False
                i += 1
                f -= 1
            elif s[i] not in letters:
                i += 1
            elif s[f] not in letters:
                f -= 1
        return True