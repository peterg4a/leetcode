class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        bmap = {'(': ')', '[': ']', '{': '}'}
        stack = []
        for l in s:
            if l == '(' or l == '[' or l == '{':
                stack.append(l)
            else:
                if stack and l == bmap[stack[-1]]:
                    stack.pop(-1)
                else:
                    return False
        return not stack
        