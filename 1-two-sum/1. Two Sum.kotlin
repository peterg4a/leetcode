class Solution {
    fun twoSum(nums: IntArray, target: Int): IntArray {
        val nummap = mutableMapOf<Int, Int>()
        nummap[nums[0]] = 0
        for (i in 1 until nums.size) {
            val diff = target - nums[i]
            if (nummap.containsKey(diff)) {
                return intArrayOf(nummap[diff]!!, i)
            }
            nummap[nums[i]] = i
        }
        return intArrayOf()
    }
}