class Solution(object):
    def twoSum(self, numbers, target):
        """
        :type numbers: List[int]
        :type target: int
        :rtype: List[int]
        """
        nummap = {}
        for i in range(len(numbers)):
            diff = target - numbers[i]
            if diff in nummap:
                return [nummap[diff]+1, i+1]
            nummap[numbers[i]] = i
        return []
        